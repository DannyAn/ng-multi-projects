{
  "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hero-app": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hero-app",
            "index": "src/hero-app/index.html",
            "main": "src/hero-app/main.ts",
            "tsConfig": "src/hero-app/../tsconfig.app.json",
            "polyfills": "src/hero-app/../_common/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/hero-app/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/hero-app",
                "output": "/"
              }
            ],
            "styles": [
              "src/hero-app/styles/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/hero-app/environments/environment.ts",
                  "with": "src/hero-app/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hero-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hero-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hero-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/hero-app/../test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/hero-app/../_common/polyfills.ts",
            "tsConfig": "src/hero-app/../tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/hero-app/styles/styles.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/hero-app/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/hero-app",
                "output": "/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hero-app-e2e": {
      "root": "",
      "projectType": "application",
      "cli": {},
      "schematics": {},
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "hero-app:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
